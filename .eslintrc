{
  parser: "babel-eslint",
  "plugins": [
    "react"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "max-len": [1, 120, 2, {ignoreComments: true}],
    "react/prop-types": [2]
  },
  "globals": {
    "window": true,
    "document": false
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"]
}

